CFLAGS = $(CCOPTS) -DKERBEROS -DKRB5 $(DEFS) $(LOCALINCLUDE)

all::

KLIB = $(TOPLIBD)/libkrb5.a $(TOPLIBD)/libcrypto.a $(ISODELIB) $(COMERRLIB) $(DBMLIB)

HESIODLIB =

zmailnotify: zmailnotify.o
	$(CC) $(CFLAGS) -o zmailnotify zmailnotify.o $(KLIB) $(HESIODLIB)

zmailnotify.o:	$(srcdir)/zmailnotify.c

all:: zmailnotify

clean::
	$(RM) zmailnotify.o poplib.o zmailnotify

install::
	cp zmailnotify ${DESTDIR}$(CLIENT_BINDIR)/zmailnotify


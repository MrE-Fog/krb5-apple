CFLAGS = $(CCOPTS) $(DEFS) $(LOCALINCLUDE)
LDFLAGS = -g

ISODELIB=@ISODELIB@
COMERRLIB=$(BUILDTOP)/util/et/libcom_err.a
DBMLIB=

all::

KLIB = $(TOPLIBD)/libkrb5.a $(TOPLIBD)/libcrypto.a $(ISODELIB) $(COMERRLIB) $(DBMLIB)

sserver: sserver.o
	$(CC) $(CFLAGS) -o sserver sserver.o $(KLIB) $(LIBS)

sserver.o:	$(srcdir)/sserver.c

all:: sserver


clean::
	$(RM) sserver.o sserver

install::
	$(INSTALL_PROGRAM) sserver ${DESTDIR}$(SERVER_BINDIR)/sserver
	$(INSTALL_DATA) sserver.M ${DESTDIR}$(SERVER_MANDIR)/sserver.8

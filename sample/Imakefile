        DEPLIBS = $(DEPKLIB)
LOCAL_LIBRARIES = $(KLIB)

CLIENTOBJS= sclient.o
SERVEROBJS= sserver.o

all:: sclient sserver

NormalProgramTarget(sclient,$(CLIENTOBJS),$(DEPLIBS),$(LOCAL_LIBRARIES),)
NormalProgramTarget(sserver,$(SERVEROBJS),$(DEPLIBS),$(LOCAL_LIBRARIES),)

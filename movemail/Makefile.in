CFLAGS = $(CCOPTS) -DKERBEROS -DKRB5 -DMAIL_USE_POP $(DEFS) $(LOCALINCLUDE)
COMERRLIB=$(BUILDTOP)/util/et/libcom_err.a
LDFLAGS = -g

OBJS = movemail.o movemail-mmdf.o

all::

KLIB = $(TOPLIBD)/libkrb5.a $(TOPLIBD)/libcrypto.a $(ISODELIB) $(COMERRLIB) $(DBMLIB)

HESIODLIB = 

movemail: movemail.o
	$(CC) $(CFLAGS) -o movemail movemail.o $(KLIB) $(HESIODLIB) $(LIBS)

all:: movemail

clean::
	$(RM) movemail

install::
	$(INSTALL_PROGRAM) movemail ${DESTDIR}$(CLIENT_BINDIR)/movemail

movemail.o:	$(srcdir)/movemail.c

